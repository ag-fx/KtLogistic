<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import rafael.logistic.view.IterationChartBi?>
<?import rafael.logistic.view.MapChartBi?>
<BorderPane prefHeight="589.0" prefWidth="1220.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <GridPane BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0"/>
                <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0"/>
                <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0"/>
                <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0"/>
                <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" valignment="BASELINE" vgrow="ALWAYS"/>
            </rowConstraints>
            <children>
                <FlowPane alignment="TOP_CENTER" GridPane.columnIndex="0">
                    <children>
                        <Label fx:id="lblA" alignment="CENTER_RIGHT" minWidth="75.0" text="α =" textAlignment="RIGHT">
                            <padding>
                                <Insets right="5.0"/>
                            </padding>
                        </Label>
                        <Spinner fx:id="spnA"/>
                    </children>
                </FlowPane>
                <FlowPane alignment="TOP_CENTER" GridPane.columnIndex="1">
                    <children>
                        <Label fx:id="lblB" alignment="CENTER_RIGHT" minWidth="75.0" text="β =" textAlignment="RIGHT">
                            <padding>
                                <Insets right="5.0"/>
                            </padding>
                        </Label>
                        <Spinner fx:id="spnB"/>
                    </children>
                </FlowPane>
                <FlowPane alignment="TOP_CENTER" GridPane.columnIndex="2">
                    <children>
                        <Label fx:id="lblx0" alignment="CENTER_RIGHT" minWidth="75.0" text="x₀ =" textAlignment="RIGHT">
                            <padding>
                                <Insets left="5.0"/>
                            </padding>
                        </Label>
                        <Spinner fx:id="spnX0"/>
                    </children>
                </FlowPane>
                <FlowPane alignment="TOP_CENTER" GridPane.columnIndex="3">
                    <children>
                        <Label fx:id="lblY0" alignment="CENTER_RIGHT" minWidth="75.0" text="y₀ =" textAlignment="RIGHT">
                            <padding>
                                <Insets left="5.0"/>
                            </padding>
                        </Label>
                        <Spinner fx:id="spnY0"/>
                    </children>
                </FlowPane>
                <FlowPane alignment="TOP_CENTER" GridPane.columnIndex="4">
                    <children>
                        <Label fx:id="lblIteractions" alignment="TOP_RIGHT" minWidth="75.0" text="Iteractions ="
                               textAlignment="RIGHT">
                            <padding>
                                <Insets right="5.0"/>
                            </padding>
                        </Label>
                        <Spinner fx:id="spnIteractions"/>
                    </children>
                </FlowPane>
            </children>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </padding>
        </GridPane>
    </top>
    <center>
        <MapChartBi fx:id="chart" animated="false" createSymbols="false" legendVisible="false"
                    BorderPane.alignment="CENTER">
            <xAxis>
                <NumberAxis autoRanging="false" lowerBound="-2.0" minorTickCount="10" side="BOTTOM" tickUnit="0.1"
                            upperBound="2.0"/>
            </xAxis>
            <yAxis>
                <NumberAxis autoRanging="false" lowerBound="-2.0" minorTickCount="10" side="LEFT" tickUnit="0.1"
                            upperBound="2.0"/>
            </yAxis>
        </MapChartBi>
    </center>
    <bottom>
        <VBox>
            <children>
                <IterationChartBi fx:id="xIterationsChart" animated="false" createSymbols="false" legendVisible="false"
                                  prefHeight="100.0" BorderPane.alignment="CENTER">
                    <xAxis>
                        <NumberAxis autoRanging="false" side="BOTTOM" tickUnit="10.0"/>
                    </xAxis>
                    <yAxis>
                        <NumberAxis autoRanging="false" side="LEFT" lowerBound="-2.0" tickUnit="0.5" upperBound="2.0"/>
                    </yAxis>
                </IterationChartBi>
                <IterationChartBi fx:id="yIterationsChart" animated="false" createSymbols="false" legendVisible="false"
                                  prefHeight="100.0" BorderPane.alignment="CENTER">
                    <xAxis>
                        <NumberAxis autoRanging="false" side="BOTTOM" tickUnit="10.0"/>
                    </xAxis>
                    <yAxis>
                        <NumberAxis autoRanging="false" side="LEFT" lowerBound="-2.0" tickUnit="0.5" upperBound="2.0"/>
                    </yAxis>
                </IterationChartBi>
            </children>
        </VBox>
    </bottom>
</BorderPane>
