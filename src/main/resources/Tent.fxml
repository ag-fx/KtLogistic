      </GridPane>
   </top>
    <center>
        <TentChart fx:id="chart" animated="false" createSymbols="false" legendVisible="false" BorderPane.alignment="CENTER">
            <xAxis>
                <NumberAxis autoRanging="false" minorTickCount="10" side="BOTTOM" tickUnit="0.1" upperBound="1.0" />
            </xAxis>
            <yAxis>
                <NumberAxis autoRanging="false" minorTickCount="10" side="LEFT" tickUnit="0.1" upperBound="1.0" />
            </yAxis>
        </TentChart>
    </center>
    <bottom>
        <IteractionChartDouble fx:id="iterationsChart" animated="false" createSymbols="false" legendVisible="false" prefHeight="100.0" BorderPane.alignment="CENTER">
            <xAxis>
                <NumberAxis fx:id="iteractionsXAxis" autoRanging="false" side="BOTTOM" tickUnit="10.0" />
            </xAxis>
            <yAxis>
                <NumberAxis autoRanging="false" side="LEFT" tickUnit="0.1" upperBound="1.0" />
            </yAxis>
        </IteractionChartDouble>
    </bottom>
