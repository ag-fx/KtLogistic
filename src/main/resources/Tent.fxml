<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <GridPane BorderPane.alignment="CENTER">
         <columnConstraints>
            <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
            <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" valignment="BASELINE" vgrow="ALWAYS" />
         </rowConstraints>
         <children>
            <FlowPane alignment="TOP_CENTER" GridPane.halignment="RIGHT">
               <children>
                  <Label fx:id="lblMi" text=" Î¼ =" textAlignment="RIGHT">
                     <padding>
                        <Insets right="5.0" />
                     </padding>
                  </Label>
                  <Spinner fx:id="spnMi" />
               </children>
            </FlowPane>
            <FlowPane alignment="TOP_CENTER" GridPane.columnIndex="1">
               <children>
                  <Label fx:id="lblx0" text="x0 =" textAlignment="RIGHT">
                     <padding>
                        <Insets left="5.0" />
                     </padding>
                  </Label>
                  <Spinner fx:id="spnX0" />
               </children>
            </FlowPane>
            <FlowPane alignment="TOP_CENTER" GridPane.columnIndex="2">
               <children>
                  <Label fx:id="lblIteractions" text="Iteractions =" textAlignment="RIGHT">
                     <padding>
                        <Insets right="5.0" />
                     </padding>
